# Production API Configuration
NEXT_PUBLIC_API_URL=https://rdc-assay-pro-production.up.railway.app/api/v1
NEXT_PUBLIC_APP_VERSION=1.0.0

# Production settings
NODE_ENV=production

# Boss Admin (server-side auth)
BOSS_KEY=administrateurx
# Optional separate signing secret (falls back to BOSS_KEY if unset)
BOSS_JWT_SECRET=change-me-to-a-strong-random-secret

# Disable boss auth (bypass token gate on admin routes)
BOSS_AUTH_DISABLED=true
